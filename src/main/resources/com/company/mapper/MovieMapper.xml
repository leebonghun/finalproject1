<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.company.mapper.MovieMapper">

<select  id="read" resultType="com.company.domain.movieDTO">
	select rank,poster,key,movieNM,actors,showTM,genres,watchGradeNm,nationNm,companyNm,openDt from movie_board where movieCD = #{movieCD}
	
	
	
</select>

<select  id="list" resultType="com.company.domain.movieDTO">
	select  * from movie_board
</select>

</mapper>